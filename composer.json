{
	"name": "esperecyan/dictionary-api",
	"description": "従来の辞書形式を「主に単語で答えるゲームにおける汎用的な辞書形式」に変換する Web API を提供します。",
	"type": "project",
	"homepage": "https://github.com/esperecyan/dictionary-api",
	"license": "MPL-2.0",
	"authors": [
		{
			"name": "100の人",
			"homepage": "https://twitter.com/esperecyan",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/esperecyan/dictionary-api/issues"
	},
	"require": {
		"php": ">=7.0",
		"bantu/ini-get-wrapper": "^1.0.1",
		"scriptfusion/byte-formatter": "^3.2.0",
		"esperecyan/dictionary-php": "^0.3.0",
		"ext-gettext": "*",
		"ext-mbstring": "*"
	},
	"require-dev": {
		"phpunit/phpunit": "^4.8.26",
		"riverline/multipart-parser": "^1.1.8",
		"h4cc/multipart": "@dev",
		"ext-xdebug": "*",
		"ext-SPL": "*",
		"ext-zip": "*"
	},
	"autoload": {
		"psr-4": {
			"esperecyan\\dictionary_api\\": "src/"
		},
		"exclude-from-classmap": ["/src/public/"]
	},
	"autoload-dev": {
		"psr-4": {
			"esperecyan\\dictionary_api\\": "tests/"
		},
		"exclude-from-classmap": ["/tests/resources/"]
	}
}
